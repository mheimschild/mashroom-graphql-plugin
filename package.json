{
  "name": "mashroom-graphql-plugin",
  "version": "1.0.0",
  "description": "Plugin adding GraphQL functionality for Mashroom Server",
  "main": "index.js",
  "scripts": {
    "build": "swc ./src/** -d ./dist",
    "lint": "eslint ./src --ext .ts",
    "type-check": "tsc --noEmit",
